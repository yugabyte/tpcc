<?xml version="1.0"?>
<parameters>
    <enableGeoPartitionedWorkload>true</enableGeoPartitionedWorkload>
    <numberOfPartitions>2</numberOfPartitions>
    <tablespaces>
        <tablespace>
            <name>tablespace0</name>
            <storeItemTable>true</storeItemTable>
            <storePartitionedTables>true</storePartitionedTables>
            <storePartitions>true</storePartitions>
            <replicationFactor>3</replicationFactor>
            <placementPolicy>
                <placementBlock>
                    <cloud>aws</cloud>
                    <region>us-west-2</region>
                    <zone>us-west-2a</zone>
                    <minReplicationFactor>3</minReplicationFactor>
                </placementBlock>
            </placementPolicy>
        </tablespace>
        <tablespace>
            <name>tablespace1</name>
            <replicationFactor>3</replicationFactor>
            <storePartitions>true</storePartitions>
            <placementPolicy>
                <placementBlock>
                    <cloud>aws</cloud>
                    <region>us-east-1</region>
                    <zone>us-east-1a</zone>
                    <minReplicationFactor>3</minReplicationFactor>
                </placementBlock>
            </placementPolicy>
        </tablespace>
    </tablespaces>
</parameters>
